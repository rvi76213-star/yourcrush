        "notify_user": false,
        "cooldown_period": 3600
    },
    "permissions": {
        "user": ["admin"],
        "pick": ["admin", "moderator"],
        "url": ["admin", "moderator"],
        "message": ["admin", "moderator"],
        "log": ["admin"]
    }
}